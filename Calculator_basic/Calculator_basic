class calculator:

	def add(a,b):
		print(a+b)

	def substract(a,b):
		print(a-b)

	def divide(a,b):
		print(a/b)

	def multiply(a,b):
		print(a*b)

a=int(input('a=?'))
b=int(input('b=?'))
action=input('what u wanna do ? ')

if action=='add': 
	calculator.add(a,b)

elif action=='sub':	
	calculator.substract(a,b)

elif action=='div':
	calculator.divide(a,b)

elif action=='mul':
	calculator.multiply(a,b)

else:
	print('Choose among (add, sub , div or mul)')
